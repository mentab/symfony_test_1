{% extends 'base.html.twig' %}

{% block body %}
    <h1>Gameobjectcategory</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ gameObjectCategory.id }}</td>
            </tr>
            <tr>
                <th>Offset</th>
                <td>{{ gameObjectCategory.offset }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ gameObjectCategory.name }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ gameObjectCategory.description }}</td>
            </tr>
            <tr>
                <th>Icon</th>
                <td>{{ gameObjectCategory.icon }}</td>
            </tr>
            <tr>
                <th>Image</th>
                <td>{{ gameObjectCategory.image }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('gameobjectcategory_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('gameobjectcategory_edit', { 'id': gameObjectCategory.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
